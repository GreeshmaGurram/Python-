sentence = list(sorted(input().split(" ")))
output = []
for i in range(len(sentence)):
    for j in range(i+1, len(sentence)):
        for k in range(j+1, len(sentence)):
            out = sentence[i] + " " + sentence[j] + " " + sentence[k]
            output.append(out)
          
for i in output:
    print(i)
            

        
